define(function(d,g,e){function c(){this.cfg={id:"",btnListId:"",btnListClass:"select",thisShow:1,time:3E3,callback:null,timer:null,ban:null,banList:null,btnList:null,nextBtn:null,prevBtn:null,autoPlay:!0};var b=this;this.onStart=function(){b.cfg.banList.stop(!0,!0).fadeOut(800).eq(b.cfg.thisShow).stop(!0,!1).fadeIn(800);b.cfg.btnListId&&b.cfg.btnList.removeClass(b.cfg.btnListClass).eq(b.cfg.thisShow).addClass(b.cfg.btnListClass);b.cfg.thisShow=b.cfg.thisShow>=b.cfg.banList.length-1?0:-1>b.cfg.thisShow?b.cfg.banList.length-1:b.cfg.thisShow+1};this.stop=function(){b.cfg.timer&&clearInterval(b.cfg.timer)};this.start=function(){b.cfg.timer=setInterval(b.onStart,b.cfg.time)};this.goto=function(a){b.stop();b.onStart();b.cfg.autoPlay&&b.start()}}d("jquery");c.prototype={init:function(b){var a=this;a.cfg=$.extend(a.cfg,b);a.cfg.ban=$(a.cfg.id);a.cfg.banList=a.cfg.ban.find("li");a.cfg.banList.hide().eq(0).show();a.cfg.btnList=$(a.cfg.btnListId).find("li");a.cfg.thisShow=1;a.stop();a.cfg.autoPlay&&a.start();1<a.cfg.banList.length?(a.cfg.btnList.removeClass(a.cfg.btnListClass).eq(0).addClass(a.cfg.btnListClass),a.cfg.btnList.each(function(b){$(this).off("click").on("click",function(f){a.cfg.thisShow=b;a.cfg.btnList.removeClass(a.cfg.btnListClass).eq(b).addClass(a.cfg.btnListClass);a.goto(a.cfg.thisShow);f.stopPropagation()})})):(a.stop(),a.cfg.btnList.remove());if(a.cfg.nextBtn)$(a.cfg.nextBtn).off("click").on("click",function(b){a.goto(a.cfg.thisShow);b.stopPropagation()});if(a.cfg.prevBtn)$(a.cfg.prevBtn).off("click").on("click",function(b){a.cfg.thisShow-=2;a.goto(a.cfg.thisShow);b.stopPropagation()});return this}};e.exports=c});